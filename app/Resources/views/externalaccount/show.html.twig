{% extends "compteBundle::layout.html.twig" %}

{% block pageContent %}
    <h1>Externalaccount</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ externalAccount.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ externalAccount.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ externalAccount.type }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('externalaccount_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('externalaccount_edit', { 'id': externalAccount.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
