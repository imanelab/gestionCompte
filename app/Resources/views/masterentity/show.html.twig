{% extends 'base.html.twig' %}

{% block body %}
    <h1>Masterentity</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ masterEntity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ masterEntity.name }}</td>
            </tr>
            <tr>
                <th>Depth</th>
                <td>{{ masterEntity.depth }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('masterentity_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('masterentity_edit', { 'id': masterEntity.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
